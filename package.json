{
  "name": "node-vue-ui-website",
  "description": "A Node Vue.js project",
  "version": "1.0.0",
  "author": "782655835@qq.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "github": "sh deploy.sh",
    "dev": "npm run dev:fed & npm run serve",
    "deploy": "npm run build:fed && git commit -am 'deploy' && git subtree push --prefix dist origin gh-pages",
    "test": "mocha --watch --recursive",
    "serve": "nodemon index.js",
    "serve:debug": "nodemon --inspect-brk=5858 index.js",
    "dev:fed": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build:fed": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "eslint --ext .js,.vue src --fix",
    "format": "prettier --write './{src,server}/**/*.{js,vue,json}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  },
  "dependencies": {
    "autoprefixer": "^9.2.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "connect-history-api-fallback": "^1.5.0",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.5.3",
    "eslint-plugin-vue": "^4.7.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "highlight.js": "^9.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "nprogress": "^0.2.0",
    "prettier": "^1.15.2",
    "vue": "^2.5.11",
    "vue-router": "^3.0.1",
    "yi-ui": "0.2.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@vusion/md-vue-loader": "^1.0.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^2.0.2",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^1.1.4",
    "mocha": "^5.2.0",
